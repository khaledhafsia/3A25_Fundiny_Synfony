{% extends 'base.html.twig' %}

{% block title %}Projet{% endblock %}

{% block body %}
    <h1>Projet</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ projet.id }}</td>
            </tr>
            <tr>
                <th>Nompr</th>
                <td>{{ projet.nompr }}</td>
            </tr>
            <tr>
                <th>Nompo</th>
                <td>{{ projet.nompo }}</td>
            </tr>
            <tr>
                <th>Dated</th>
                <td>{{ projet.dated ? projet.dated|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Ca</th>
                <td>{{ projet.ca }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_projet_index') }}">back to list</a>

    <a href="{{ path('app_projet_edit', {'id': projet.id}) }}">edit</a>

    {{ include('front/projet/_delete_form.html.twig') }}
{% endblock %}
